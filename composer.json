{
    "name": "widoz/bem",
    "description": "Bem convention as class",
    "type": "library",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Guido Scialfa",
            "email": "dev@guidoscialfa.com"
        }
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/widoz/bem",
    "support": {
        "issues": "https://github.com/widoz/bem/issues"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Widoz\\BemScope\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Widoz\\Bem\\Tests\\": "tests/",
            "Widoz\\Bem\\Unit\\": "tests/unit/"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.5",
        "brain/monkey": "dev-master",
        "automattic/phpcs-neutron-standard": "~1.2.0",
        "wp-coding-standards/wpcs": "~0.14.0",
        "object-calisthenics/phpcs-calisthenics-rules": "~2.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
